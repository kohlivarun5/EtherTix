{
  "name": "smarttix",
  "version": "1.0.0",
  "description": "Smart Tix Dapp",
  "main": "index.js",
  "scripts": {
    "remix": "remixd -s $PWD/sol/ --remix-ide https://remix.ethereum.org",
    "solcjs": "solcjs --abi sol/*.sol -o build && solcjs --optimize --bin sol/*.sol -o build",
    "solc": "solc --overwrite --abi sol/*.sol -o build && solc --optimize --bin --overwrite sol/*.sol -o build && solc --link --libraries sol/libraries.txt build/*.bin",
    "build": "bsb -make-world",
    "start": "bsb -make-world -w",
    "clean": "bsb -clean-world",
    "webpack": "webpack -w --content-base=web",
    "server": "webpack-dev-server",
    "geth": "geth --dev --datadir build/geth --rpc",
    "webpack:production": "NODE_ENV=production webpack",
    "release": "make release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kohlivarun5/SmartTix.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kohlivarun5/SmartTix/issues"
  },
  "homepage": "https://github.com/kohlivarun5/SmartTix#readme",
  "devDependencies": {
    "@remix-project/remixd": "^0.3.2",
    "bs-platform": "^4.0.3",
    "eth-json-rpc-filters": "^4.1.1",
    "json-rpc-engine": "^5.4.0",
    "readable-stream": "^2.3.7",
    "solc": "^0.6.12",
    "stream-browserify": "^3.0.0",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.1",
    "webpack-dev-server": "^3.1.5"
  },
  "dependencies": {
    "@walletconnect/web3-provider": "^1.3.6",
    "qrcode": "^1.2.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-responsive-carousel": "^3.1.43",
    "reason-react": "^0.5.3",
    "web3": "^1.3.4",
    "web3modal": "^1.9.3"
  }
}
